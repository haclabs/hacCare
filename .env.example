# Supabase Configuration - Copy to .env and update with your credentials
# Get your keys from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=sb_publishable_your_anon_key_here

# Application Configuration
VITE_APP_VERSION=5.0.0
VITE_DEBUG_MODE=false

# Optional: Enable BCMA Debug Mode
VITE_DEBUG_BARCODE=false

# Optional: Security Diagnostics
VITE_ENABLE_SECURITY_DIAGNOSTICS=true
