# Production Environment Variables
# Update these values and add them to your Netlify environment variables

# Application Environment
NODE_ENV=production
VITE_PRODUCTION_DOMAIN=yourdomain.com

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Subdomain Configuration
VITE_SUBDOMAIN_ROUTING_ENABLED=true
VITE_DEFAULT_SUBDOMAIN=www

# Application Configuration
VITE_APP_NAME="Your Healthcare App"
VITE_SUPPORT_EMAIL=support@yourdomain.com
VITE_API_BASE_URL=https://api.yourdomain.com

# Security
VITE_ENABLE_HTTPS_REDIRECT=true
VITE_ENABLE_CSP=true

# Debug (set to false in production)
VITE_DEBUG_MODE=false
VITE_DEBUG_BARCODE=false
VITE_ENABLE_SECURITY_DIAGNOSTICS=false
