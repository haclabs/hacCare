{
  "name": "haccare",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "description": "Advanced Healthcare Management System with BCMA barcode medication administration, diabetic care, security diagnostics, and comprehensive patient management",
  "keywords": [
    "healthcare",
    "patient-management",
    "bcma",
    "barcode-scanner",
    "medication-administration",
    "diabetic-care",
    "security-diagnostics",
    "medical",
    "react",
    "typescript"
  ],
  "author": "hacCare Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "./node_modules/.bin/vite build",
    "build:fallback": "npx --package=typescript tsc && npx --package=vite vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "./node_modules/.bin/tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist node_modules/.vite",
    "deps:update": "ncu -u && npm install",
    "deps:check": "ncu",
    "postinstall": "echo 'Dependencies installed successfully'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.17.0",
    "@types/jsbarcode": "^3.11.4",
    "camelcase-css": "^2.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "jsbarcode": "^3.12.1",
    "lucide-react": "^0.312.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-image-marker": "^1.2.0",
    "react-router-dom": "^6.21.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-check-updates": "^16.14.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
